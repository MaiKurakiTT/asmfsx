<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical"    android:background="#fff"     >         <include        android:id="@+id/il_see_titlebar"        android:layout_width="fill_parent"        android:layout_height="wrap_content"        layout="@layout/il_title" />        <View        android:layout_width="fill_parent"        android:layout_height="15dp"        android:background="@color/view_module" />    <RelativeLayout        android:layout_width="fill_parent"        android:layout_height="50dp" >        <ImageView             android:id="@+id/img_register_phone"            android:layout_width="25dp"            android:layout_height="25dp"            android:layout_alignParentLeft="true"            android:layout_marginLeft="20dp"            android:layout_centerVertical="true"            android:background="@drawable/ic_phone_80"            />        <EditText            android:id="@+id/it_register_username"            android:layout_width="200dp"            android:layout_height="wrap_content"            android:layout_toRightOf="@id/img_register_phone"            android:layout_marginLeft="20dp"            android:layout_centerVertical="true"            android:background="#ffffff"            android:hint="输入手机号"            android:inputType="number"            android:maxLength="11" />            </RelativeLayout>    <View        android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_module" />    <RelativeLayout        android:layout_width="fill_parent"        android:layout_height="50dp" >        <ImageView             android:id="@+id/img_register_code"            android:layout_width="25dp"            android:layout_height="25dp"            android:layout_alignParentLeft="true"            android:layout_marginLeft="20dp"            android:layout_centerVertical="true"            android:background="@drawable/ic_code_80"            />                <EditText            android:id="@+id/it_register_code"            android:layout_width="180dp"            android:layout_height="wrap_content"            android:layout_centerVertical="true"            android:layout_marginLeft="20dp"            android:layout_toRightOf="@id/img_register_code"            android:background="#ffffff"            android:hint="输入验证码"            android:inputType="number" />        <RelativeLayout             android:id="@+id/but_regist_getcode"            android:layout_width="100dp"            android:layout_height="50dp"            android:background="@drawable/layoutclick_style"            android:clickable="true"            android:layout_alignParentRight="true"                        >            <TextView                 android:id="@+id/ii_getcode"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="获取验证码"                android:layout_centerInParent="true"                android:textColor="@color/it_normal_textcolor"                />        </RelativeLayout>    </RelativeLayout>    <View        android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_module" />    <RelativeLayout        android:layout_width="fill_parent"        android:layout_height="50dp" >         <ImageView             android:id="@+id/img_register_lock"            android:layout_width="25dp"            android:layout_height="25dp"            android:layout_alignParentLeft="true"            android:layout_marginLeft="20dp"            android:layout_centerVertical="true"            android:background="@drawable/ic_lock_80"            />        <EditText            android:id="@+id/it_register_password"            android:layout_width="fill_parent"            android:layout_height="wrap_content"            android:layout_toRightOf="@id/img_register_lock"            android:layout_marginLeft="20dp"            android:layout_centerVertical="true"            android:background="#ffffff"            android:password="true"            android:hint="输入密码" />           </RelativeLayout>    <View        android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_module" />    <!-- <RelativeLayout        android:id="@+id/il_regist_gender"        android:layout_width="fill_parent"        android:layout_height="50dp"        android:clickable="true"        android:background="@drawable/layoutclick_style" >        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_centerVertical="true"            android:layout_marginLeft="20dp"            android:text="性别" />        <TextView            android:id="@+id/it_regist_gender"            android:layout_width="250dp"            android:layout_height="wrap_content"            android:layout_alignParentRight="true"            android:layout_centerVertical="true"            android:hint="输入性别"            android:clickable="true" />    </RelativeLayout> -->     <View        android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_fill" />     <RelativeLayout             android:id="@+id/but_regist_register"            android:layout_width="300dp"            android:layout_height="50dp"            android:background="@drawable/button_style_green"            android:clickable="true"            android:layout_gravity="center_horizontal"            android:layout_marginTop="50dp"            >            <TextView                 android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="注册"                android:layout_centerInParent="true"                />        </RelativeLayout>      </LinearLayout>