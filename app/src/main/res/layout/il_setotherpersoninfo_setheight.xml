<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical" >    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="vertical" >        <include             layout="@layout/il_title"            android:layout_width="fill_parent"            android:layout_height="wrap_content"            />        <View            android:layout_width="fill_parent"            android:layout_height="15dp"            android:background="#11000000" />        <RelativeLayout            android:layout_width="fill_parent"            android:layout_height="45dp" >            <TextView                android:id="@+id/id_setotherpersoninfo_setheight_matchheight"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerVertical="true"                android:layout_marginLeft="15dp"                android:text="身高"                android:textColor="#88000000"                android:textSize="15sp" />            <TextView                android:id="@+id/id_setotherpersoninfo_setheight_height"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentLeft="true"                android:layout_centerVertical="true"                android:layout_marginLeft="120dp"                android:text="160cm"                android:textSize="15sp" />        </RelativeLayout>        <View            android:layout_width="fill_parent"            android:layout_height="0.1dp"            android:background="#66000000" />    </LinearLayout>    <RelativeLayout        android:id="@+id/il_setotherpersoninfo_setheight_choose"        android:layout_width="fill_parent"        android:layout_height="wrap_content"        android:layout_alignParentBottom="true" >    </RelativeLayout></RelativeLayout>