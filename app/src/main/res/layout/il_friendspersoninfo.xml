<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical" >    <include         layout="@layout/il_title"        android:layout_width="fill_parent"        android:layout_height="wrap_content"        />    <View        android:layout_width="fill_parent"        android:layout_height="15dp"        android:background="#11000000" />    <RelativeLayout        android:id="@+id/id_foufri_item_personinfo_personinfo"        android:layout_width="fill_parent"        android:layout_height="wrap_content"        android:background="@drawable/layoutclick_style"        android:clickable="true" >        <com.hsd.fsxasm.widget.CircleImageView            android:id="@+id/id_foufri_item_personinfo_head"            android:layout_width="60dp"            android:layout_height="60dp"            android:layout_centerVertical="true"            android:layout_marginLeft="8dp"            android:layout_marginTop="8dp"            android:src="@drawable/ic_inithead"            app:border_color="@color/circleview_white" />        <TextView            android:id="@+id/id_foufri_item_personinfo_name"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginLeft="20dp"            android:layout_marginTop="10dp"            android:layout_toRightOf="@id/id_foufri_item_personinfo_head"            android:text="萌萌"            android:textSize="20sp" />        <TextView            android:id="@+id/id_foufri_item_personinfo_affectivestate"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignLeft="@id/id_foufri_item_personinfo_name"            android:layout_below="@id/id_foufri_item_personinfo_name"            android:text="恋爱状态"            android:textColor="#88000000"            android:textSize="15sp" />        <TextView            android:id="@+id/id_foufri_item_personinfo_affectivestateinfo"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_below="@id/id_foufri_item_personinfo_name"            android:layout_marginLeft="5dp"            android:layout_toRightOf="@id/id_foufri_item_personinfo_affectivestate"            android:text="单身"            android:textColor="#88000000"            android:textSize="15sp" />        <ImageView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentRight="true"            android:layout_marginRight="20dp"            android:layout_centerVertical="true"            android:src="@drawable/ic_advance" />    </RelativeLayout>    <View        android:layout_width="fill_parent"        android:layout_height="15dp"        android:background="#11000000" />    <LinearLayout        android:id="@+id/id_foufri_item_personinfo_photo"        android:layout_width="fill_parent"        android:layout_height="80dp"        android:background="@drawable/layoutclick_style"        android:clickable="true"        android:orientation="horizontal" >        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_gravity="center_vertical"            android:layout_marginLeft="15dp"            android:text="照片墙"            android:textColor="#88000000"            android:textSize="16sp" />        <ImageView            android:id="@+id/id_foufri_item_personinfo_photo1"            android:layout_width="60dp"            android:layout_height="60dp"            android:layout_gravity="center_vertical"            android:layout_marginLeft="25dp"            android:src="@drawable/photo1" />        <ImageView            android:id="@+id/id_foufri_item_personinfo_photo2"            android:layout_width="60dp"            android:layout_height="60dp"            android:layout_gravity="center_vertical"            android:layout_marginLeft="10dp"            android:src="@drawable/photo2" />        <ImageView            android:id="@+id/id_foufri_item_personinfo_photo3"            android:layout_width="60dp"            android:layout_height="60dp"            android:layout_gravity="center_vertical"            android:layout_marginLeft="10dp"            android:src="@drawable/photo3" />    </LinearLayout>    <View        android:layout_width="fill_parent"        android:layout_height="15dp"        android:background="#11000000" />    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="200dp" >       <!--  <ImageView            android:id="@+id/id_foufri_item_personinfo_matchname_ic"            android:layout_width="20dp"            android:layout_height="21dp"            android:layout_marginLeft="15dp"            android:layout_marginTop="8dp"            android:src="@drawable/ic_match_name_icon" /> -->        <TextView            android:id="@+id/id_foufri_item_personinfo_matchname"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginLeft="15dp"            android:layout_marginTop="8dp"            android:text="个人信息"            android:textColor="#88000000"            android:textSize="16sp" />        <View            android:id="@+id/id_foufri_item_personinfo_view3"            android:layout_width="300dp"            android:layout_height="0.2dp"            android:layout_below="@id/id_foufri_item_personinfo_matchname"            android:layout_centerHorizontal="true"            android:layout_marginTop="8dp"            android:background="#11000000" />        <TextView            android:id="@+id/id_foufri_item_personinfo_matchsex"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_below="@id/id_foufri_item_personinfo_view3"            android:layout_marginLeft="50dp"            android:layout_marginTop="10dp"            android:text="性别"            android:textColor="#88000000"            android:textSize="15sp" />        <TextView            android:id="@+id/id_foufri_item_personinfo_sex"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignTop="@id/id_foufri_item_personinfo_matchsex"            android:layout_marginLeft="35dp"            android:layout_toRightOf="@id/id_foufri_item_personinfo_matchsex"            android:text="女"            android:textSize="15sp" />                <TextView            android:id="@+id/id_foufri_item_personinfo_matchbirthday"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignLeft="@id/id_foufri_item_personinfo_matchsex"            android:layout_below="@id/id_foufri_item_personinfo_matchsex"            android:layout_marginTop="8dp"            android:text="生日"            android:textColor="#88000000"            android:textSize="15sp" />        <TextView            android:id="@+id/id_foufri_item_personinfo_birthday"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignLeft="@id/id_foufri_item_personinfo_sex"            android:layout_alignTop="@id/id_foufri_item_personinfo_matchbirthday"            android:layout_below="@id/id_foufri_item_personinfo_sex"            android:layout_toRightOf="@id/id_foufri_item_personinfo_matchbirthday"            android:text="1995-01-01"            android:textSize="15sp" />        <TextView            android:id="@+id/id_foufri_item_personinfo_matchstar"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignLeft="@id/id_foufri_item_personinfo_matchsex"            android:layout_below="@id/id_foufri_item_personinfo_birthday"            android:layout_marginTop="8dp"            android:text="星座"            android:textColor="#88000000"            android:textSize="15sp" />        <TextView            android:id="@+id/id_foufri_item_personinfo_star"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignLeft="@id/id_foufri_item_personinfo_sex"            android:layout_alignTop="@id/id_foufri_item_personinfo_matchstar"            android:layout_below="@id/id_foufri_item_personinfo_sex"            android:layout_toRightOf="@id/id_foufri_item_personinfo_matchstar"            android:text="水瓶座"            android:textSize="15sp" />        <TextView            android:id="@+id/id_foufri_item_personinfo_matchschool"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignLeft="@id/id_foufri_item_personinfo_matchsex"            android:layout_below="@id/id_foufri_item_personinfo_matchstar"            android:layout_marginTop="8dp"            android:text="学校"            android:textColor="#88000000"            android:textSize="15sp" />        <TextView            android:id="@+id/id_foufri_item_personinfo_school"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignLeft="@id/id_foufri_item_personinfo_sex"            android:layout_alignTop="@id/id_foufri_item_personinfo_matchschool"            android:layout_below="@id/id_foufri_item_personinfo_star"            android:layout_toRightOf="@id/id_foufri_item_personinfo_matchschool"            android:maxWidth="250dp"            android:maxHeight="80dp"            android:maxLength="50"            android:text="河南师范大学"            android:textSize="15sp" />                <TextView            android:id="@+id/id_foufri_item_personinfo_matchlocation"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignLeft="@id/id_foufri_item_personinfo_matchsex"            android:layout_below="@id/id_foufri_item_personinfo_matchschool"            android:layout_marginTop="8dp"            android:text="家乡"            android:textColor="#88000000"            android:textSize="15sp" />        <TextView            android:id="@+id/id_foufri_item_personinfo_location"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignLeft="@id/id_foufri_item_personinfo_sex"            android:layout_alignTop="@id/id_foufri_item_personinfo_matchlocation"            android:layout_below="@id/id_foufri_item_personinfo_school"            android:layout_toRightOf="@id/id_foufri_item_personinfo_matchlocation"            android:maxWidth="250dp"            android:maxHeight="80dp"            android:maxLength="50"            android:text="河南 新乡"            android:textSize="15sp" />            </RelativeLayout>    <RelativeLayout        android:layout_width="fill_parent"        android:layout_height="fill_parent"        android:background="#11000000" >           </RelativeLayout></LinearLayout>