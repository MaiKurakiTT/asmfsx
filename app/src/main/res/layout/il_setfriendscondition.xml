<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical" >	    <include         layout="@layout/il_title"        android:layout_width="fill_parent"        android:layout_height="wrap_content"        />    <View         android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_module"        />    <View         android:layout_width="fill_parent"        android:layout_height="10dp"        android:background="@color/view_module"        />    <View         android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_module"        />    <RelativeLayout         android:id="@+id/il_setfriendscondition_age"        android:layout_width="fill_parent"        android:layout_height="50dp"        android:background="@drawable/layoutclick_style"            android:clickable="true"        >        <TextView             android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="年龄范围"            android:textColor="@color/it_normal_textcolor"            android:textSize="18dp"            android:layout_centerVertical="true"            android:layout_marginLeft="10dp"            />        <LinearLayout            android:layout_width="100dp"             android:layout_height="wrap_content"            android:orientation="horizontal"            android:layout_centerVertical="true"            android:layout_alignParentRight="true"            android:layout_marginRight="50dp">        <TextView             android:id="@+id/it_setfriendscondition_agemin"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="1"            android:text="18"            android:textColor="@color/it_match_textcolor"            android:textSize="18dp"            android:gravity="center_horizontal"            />        <TextView             android:id="@+id/it_setfriendscondition_agecenter"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="1"            android:text="~"            android:textColor="@color/it_match_textcolor"            android:textSize="18dp"            android:gravity="center_horizontal"            />        <TextView             android:id="@+id/it_setfriendscondition_agemax"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="1"            android:text="20"            android:textColor="@color/it_match_textcolor"            android:textSize="18dp"            android:gravity="center_horizontal"            />        </LinearLayout>        <ImageView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentRight="true"                android:layout_centerVertical="true"                android:layout_marginRight="20dp"                android:src="@drawable/ic_advance" />    </RelativeLayout>   <View         android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_notfill"        />   <RelativeLayout         android:id="@+id/il_setfriendscondition_height"        android:layout_width="fill_parent"        android:layout_height="50dp"        android:background="@drawable/layoutclick_style"            android:clickable="true"        >        <TextView             android:id="@+id/it_setfriendscondition_matchheight"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="身高范围"            android:textColor="@color/it_normal_textcolor"            android:textSize="18dp"            android:layout_centerVertical="true"            android:layout_marginLeft="10dp"            />        <LinearLayout            android:layout_width="150dp"             android:layout_height="wrap_content"            android:orientation="horizontal"            android:layout_centerVertical="true"            android:layout_alignParentRight="true"            android:layout_marginRight="50dp">        <TextView             android:id="@+id/it_setfriendscondition_heightmin"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="3"            android:text="160cm"            android:textColor="@color/it_match_textcolor"            android:textSize="18dp"            android:gravity="center_horizontal"            />         <TextView             android:id="@+id/it_setfriendscondition_height1"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="1"            android:text="~"            android:textColor="@color/it_match_textcolor"            android:textSize="18dp"            android:gravity="center_horizontal"            />          <TextView             android:id="@+id/it_setfriendscondition_heightmax"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="3"            android:text="165cm"            android:textColor="@color/it_match_textcolor"            android:textSize="18dp"            android:gravity="center_horizontal"            />        </LinearLayout>        <ImageView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentRight="true"                android:layout_centerVertical="true"                android:layout_marginRight="20dp"                android:src="@drawable/ic_advance" />    </RelativeLayout>   <View         android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_notfill"        />   <RelativeLayout         android:id="@+id/il_setfriendscondition_school"        android:layout_width="fill_parent"        android:layout_height="50dp"        android:background="@drawable/layoutclick_style"            android:clickable="true"        >        <TextView             android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="学校名称"            android:textColor="@color/it_normal_textcolor"            android:textSize="18dp"            android:layout_centerVertical="true"            android:layout_marginLeft="10dp"            />        <TextView             android:id="@+id/it_setfriendscondition_school"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="河南师范大学"            android:textColor="@color/it_match_textcolor"            android:textSize="18dp"            android:layout_centerVertical="true"            android:layout_alignParentRight="true"            android:layout_marginRight="50dp"            />        <ImageView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentRight="true"                android:layout_centerVertical="true"                android:layout_marginRight="20dp"                android:src="@drawable/ic_advance" />    </RelativeLayout>   <View         android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_notfill"        />   <RelativeLayout         android:id="@+id/il_setfriendscondition_location"        android:layout_width="fill_parent"        android:layout_height="50dp"        android:background="@drawable/layoutclick_style"            android:clickable="true"        >        <TextView             android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="家乡"            android:textColor="@color/it_normal_textcolor"            android:textSize="18dp"            android:layout_centerVertical="true"            android:layout_marginLeft="10dp"            />        <TextView             android:id="@+id/it_setfriendscondition_location"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="河南 新乡 牧野区"            android:textColor="@color/it_match_textcolor"            android:textSize="18dp"            android:layout_centerVertical="true"            android:layout_alignParentRight="true"            android:layout_marginRight="50dp"            />        <ImageView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentRight="true"                android:layout_centerVertical="true"                android:layout_marginRight="20dp"                android:src="@drawable/ic_advance" />    </RelativeLayout>   <View         android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_notfill"        />   <RelativeLayout         android:id="@+id/il_setfriendscondition_ok"        android:layout_width="300dp"        android:layout_height="50dp"         android:background="@drawable/button_style_green"            android:clickable="true"            android:layout_marginTop="30dp"            android:layout_gravity="center_horizontal"        >        <TextView             android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="完成"            android:textColor="@color/it_normal_textcolor"            android:textSize="18dp"            android:layout_centerInParent="true"            />            </RelativeLayout></LinearLayout>