<?xml version="1.0" encoding="utf-8"?><ScrollView xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical"         android:background="#fff"    >    <LinearLayout        android:id="@+id/il_settingafterregister_parent"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:orientation="vertical" >        <RelativeLayout             android:layout_width="fill_parent"            android:layout_height="45dp"            android:background="#3EA9FA"            >            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="设置个人信息"                android:textSize="15sp"                android:textColor="#ffffff"                android:layout_centerInParent="true"                 />        </RelativeLayout>       <!--  <View            android:layout_width="fill_parent"            android:layout_height="10dp"            android:background="@color/view_module" /> -->        <RelativeLayout            android:id="@+id/il_settingafterregister_sethead"            android:layout_width="fill_parent"            android:layout_height="120dp"            android:background="#ffffff"             >            <com.hsd.fsxasm.widget.CircleImageView                android:id="@+id/ic_settingafterregister_head"                android:layout_width="100dp"                android:layout_height="100dp"                android:layout_centerVertical="true"                android:layout_marginLeft="20dp"                android:src="@drawable/ic_inithead"                app:border_color="@color/circleview_white"                android:clickable="true" />            <EditText                android:id="@+id/it_settingafterregister_id"                android:layout_width="200dp"                android:layout_height="50dp"                android:layout_marginLeft="20dp"                android:layout_marginTop="10dp"                android:layout_toRightOf="@id/ic_settingafterregister_head"                android:background="#ffffff"                android:enabled="false" />            <View                android:id="@+id/it_settingafterregister_idandnameview"                android:layout_width="200dp"                android:layout_height="0.1dp"                android:layout_alignLeft="@id/it_settingafterregister_id"                android:layout_below="@id/it_settingafterregister_id"                android:background="@color/view_module" />            <EditText                android:id="@+id/it_settingafterregister_name"                android:layout_width="200dp"                android:layout_height="50dp"                android:layout_alignLeft="@id/it_settingafterregister_id"                android:layout_below="@id/it_settingafterregister_idandnameview"                android:background="#ffffff"                android:hint="输入昵称" />        </RelativeLayout>        <View            android:layout_width="fill_parent"            android:layout_height="0.1dp"            android:background="@color/view_module" />        <View            android:layout_width="fill_parent"            android:layout_height="0.1dp"            android:background="@color/view_module" />        <RelativeLayout            android:id="@+id/il_settingafterregister_setsex"            android:layout_width="fill_parent"            android:layout_height="50dp"            android:background="@drawable/layoutclick_style"            android:clickable="true" >            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerVertical="true"                android:layout_marginLeft="20dp"                android:text="@string/it_settingafterregister_setsex"                android:textColor="@color/it_normal_textcolor" />            <TextView                android:id="@+id/it_settingafterregister_sex"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerVertical="true"                android:layout_marginLeft="150dp"                android:textColor="@color/it_match_textcolor" />            <ImageView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentRight="true"                android:layout_centerVertical="true"                android:layout_marginRight="20dp"                android:src="@drawable/ic_advance" />        </RelativeLayout>        <View            android:layout_width="fill_parent"            android:layout_height="0.1dp"            android:background="@color/view_module" />        <RelativeLayout            android:id="@+id/il_settingafterregister_setbirthday"            android:layout_width="fill_parent"            android:layout_height="50dp"            android:background="@drawable/layoutclick_style"            android:clickable="true" >            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerVertical="true"                android:layout_marginLeft="20dp"                android:text="@string/it_settingafterregister_setbirthday"                android:textColor="@color/it_normal_textcolor" />            <TextView                android:id="@+id/it_settingafterregister_birthday"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerVertical="true"                android:layout_marginLeft="150dp"                android:textColor="@color/it_match_textcolor" />            <ImageView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentRight="true"                android:layout_centerVertical="true"                android:layout_marginRight="20dp"                android:src="@drawable/ic_advance" />        </RelativeLayout>        <View            android:layout_width="fill_parent"            android:layout_height="0.1dp"            android:background="@color/view_module" />        <RelativeLayout            android:id="@+id/il_settingafterregister_setheight"            android:layout_width="fill_parent"            android:layout_height="50dp"            android:background="@drawable/layoutclick_style"            android:clickable="true" >            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerVertical="true"                android:layout_marginLeft="20dp"                android:text="@string/it_settingafterregister_setheight"                android:textColor="@color/it_normal_textcolor" />            <TextView                android:id="@+id/it_settingafterregister_height"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerVertical="true"                android:layout_marginLeft="150dp"                android:textColor="@color/it_match_textcolor" />            <ImageView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentRight="true"                android:layout_centerVertical="true"                android:layout_marginRight="20dp"                android:src="@drawable/ic_advance" />        </RelativeLayout>        <View            android:layout_width="fill_parent"            android:layout_height="0.1dp"            android:background="@color/view_module" />        <RelativeLayout            android:id="@+id/il_settingafterregister_sethome"            android:layout_width="fill_parent"            android:layout_height="50dp"            android:background="@drawable/layoutclick_style"            android:clickable="true" >            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerVertical="true"                android:layout_marginLeft="20dp"                android:text="@string/it_settingafterregister_sethome"                android:textColor="@color/it_normal_textcolor" />            <TextView                android:id="@+id/it_settingafterregister_home"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerVertical="true"                android:layout_marginLeft="150dp"                android:textColor="@color/it_match_textcolor" />            <ImageView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentRight="true"                android:layout_centerVertical="true"                android:layout_marginRight="20dp"                android:src="@drawable/ic_advance" />        </RelativeLayout>        <View            android:layout_width="fill_parent"            android:layout_height="0.1dp"            android:background="@color/view_module" />        <RelativeLayout            android:id="@+id/il_settingafterregister_setschool"            android:layout_width="fill_parent"            android:layout_height="50dp"            android:background="@drawable/layoutclick_style"            android:clickable="true" >            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerVertical="true"                android:layout_marginLeft="20dp"                android:text="@string/it_settingafterregister_setschool"                android:textColor="@color/it_normal_textcolor" />            <TextView                android:id="@+id/it_settingafterregister_school"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerVertical="true"                android:layout_marginLeft="150dp"                android:textColor="@color/it_match_textcolor" />            <ImageView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentRight="true"                android:layout_centerVertical="true"                android:layout_marginRight="20dp"                android:src="@drawable/ic_advance" />        </RelativeLayout>        <View            android:layout_width="fill_parent"            android:layout_height="0.1dp"            android:background="@color/view_module" />        <RelativeLayout            android:id="@+id/il_settingafterregister_setemotion"            android:layout_width="fill_parent"            android:layout_height="50dp"            android:background="@drawable/layoutclick_style"            android:clickable="true" >            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerVertical="true"                android:layout_marginLeft="20dp"                android:text="@string/it_settingafterregister_setemotion"                android:textColor="@color/it_normal_textcolor" />            <TextView                android:id="@+id/it_settingafterregister_emotion"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerVertical="true"                android:layout_marginLeft="150dp"                android:text="@string/it_settingafterregister_emotion"                android:textColor="@color/it_match_textcolor" />            <ImageView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentRight="true"                android:layout_centerVertical="true"                android:layout_marginRight="20dp"                android:src="@drawable/ic_advance" />        </RelativeLayout>        <View            android:layout_width="fill_parent"            android:layout_height="0.1dp"            android:background="@color/view_fill" />        <RelativeLayout            android:id="@+id/il_settingafterregister_ok"            android:layout_width="300dp"            android:layout_height="50dp"            android:background="@drawable/button_style_green"             android:layout_gravity="center_horizontal"            android:layout_marginTop="30dp"            android:clickable="true" >            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerInParent="true"                android:text="@string/it_settingafterregister_ok"                android:textColor="@color/it_normal_textcolor" />        </RelativeLayout>                <RelativeLayout             android:layout_width="fill_parent"            android:layout_height="50dp"            ></RelativeLayout>    </LinearLayout></ScrollView>