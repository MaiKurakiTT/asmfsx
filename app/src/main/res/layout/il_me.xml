<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical" >        <include        android:id="@+id/title_bar"        android:layout_width="match_parent"        android:layout_height="wrap_content"        layout="@layout/il_title" />            <RelativeLayout        android:id="@+id/il_personal_topbg"        android:layout_width="fill_parent"        android:layout_height="130dp"        android:padding="5dp"        android:background="@drawable/jianbian2"        android:clickable="true" >        <com.hsd.fsxasm.widget.CircleImageView            android:id="@+id/ic_il_personal_head"            android:layout_width="100dp"            android:layout_height="100dp"            android:src="@drawable/ic_inithead"            android:layout_marginLeft="10dp"            android:layout_marginTop="8dp"            app:border_color="@color/circleview_white"             />        <TextView             android:id="@+id/il_personal_name"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="昵称"            android:textSize="@dimen/it_il_personal_name"            android:textColor="@color/it_normal_textcolor"            android:layout_toRightOf="@id/ic_il_personal_head"            android:layout_marginLeft="10dp"            android:layout_marginTop="30dp"            />        <TextView             android:id="@+id/il_personal_gender"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="@string/it_il_personal_gender"            android:textSize="@dimen/it_il_personal_namebelow"            android:textColor="@color/it_il_personal_namebelow_textcolor"            android:layout_alignLeft="@id/il_personal_name"            android:layout_below="@id/il_personal_name"            android:layout_marginTop="5dp"            />        <TextView             android:id="@+id/il_personal_age"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="1岁"            android:textSize="@dimen/it_il_personal_namebelow"            android:textColor="@color/it_il_personal_namebelow_textcolor"            android:layout_alignTop="@id/il_personal_gender"            android:layout_toRightOf="@id/il_personal_gender"            android:layout_marginLeft="5dp"            />        <TextView             android:id="@+id/il_personal_school"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="学校"            android:textSize="@dimen/it_il_personal_namebelow"            android:textColor="@color/it_il_personal_namebelow_textcolor"            android:layout_alignTop="@id/il_personal_gender"            android:layout_toRightOf="@id/il_personal_age"            android:layout_marginLeft="5dp"            />        <ImageView             android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:src="@drawable/ic_advance"            android:layout_alignParentRight="true"            android:layout_centerVertical="true"            android:layout_marginRight="20dp"            />    </RelativeLayout>    <View        android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_fill" />    <RelativeLayout        android:id="@+id/il_personal_addfriends"        android:layout_width="fill_parent"        android:layout_height="45dp"        android:clickable="true"        android:background="@drawable/layoutclick_style" >        <ImageView            android:id="@+id/ic_personal_addfriends"            android:layout_width="25dp"            android:layout_height="25dp"            android:layout_centerVertical="true"            android:layout_marginLeft="10dp"            android:src="@drawable/ic_addfriends" />        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentLeft="true"            android:layout_centerVertical="true"            android:layout_marginLeft="70dp"            android:text="@string/it_il_personal_addfriends"            android:textColor="@color/it_normal_textcolor"            android:textSize="@dimen/it_il_personal_list" />        <ImageView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentRight="true"            android:layout_centerVertical="true"            android:layout_marginRight="15dp"            android:src="@drawable/ic_advance" />    </RelativeLayout>    <View        android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_fill" />    <View        android:layout_width="fill_parent"        android:layout_height="10dp"        android:background="@color/view_module" />    <View        android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_fill" />        <RelativeLayout        android:id="@+id/il_personal_circlefriends"        android:layout_width="fill_parent"        android:layout_height="45dp"        android:clickable="true"        android:background="@drawable/layoutclick_style" >        <ImageView            android:layout_width="25dp"            android:layout_height="25dp"            android:layout_centerVertical="true"            android:layout_marginLeft="10dp"            android:src="@drawable/ic_circlefriends" />        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentLeft="true"            android:layout_centerVertical="true"            android:layout_marginLeft="70dp"            android:text="@string/it_il_personal_circlefriends"            android:textColor="@color/it_normal_textcolor"            android:textSize="@dimen/it_il_personal_list" />        <ImageView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentRight="true"            android:layout_centerVertical="true"            android:layout_marginRight="15dp"            android:src="@drawable/ic_advance" />    </RelativeLayout>    <View        android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_fill" />    <RelativeLayout        android:id="@+id/il_personal_appointment"        android:layout_width="fill_parent"        android:layout_height="45dp"        android:clickable="true"        android:background="@drawable/layoutclick_style" >        <ImageView            android:layout_width="25dp"            android:layout_height="25dp"            android:layout_centerVertical="true"            android:layout_marginLeft="10dp"            android:src="@drawable/ic_appointment" />        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentLeft="true"            android:layout_centerVertical="true"            android:layout_marginLeft="70dp"            android:text="@string/it_il_personal_appointment"            android:textColor="@color/it_normal_textcolor"            android:textSize="@dimen/it_il_personal_list" />        <ImageView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentRight="true"            android:layout_centerVertical="true"            android:layout_marginRight="15dp"            android:src="@drawable/ic_advance" />    </RelativeLayout>    <View        android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_fill" />    <View        android:layout_width="fill_parent"        android:layout_height="10dp"        android:background="@color/view_module" />    <View        android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_fill" />    <RelativeLayout        android:id="@+id/il_personal_setting"        android:layout_width="fill_parent"        android:layout_height="45dp"        android:clickable="true"        android:background="@drawable/layoutclick_style" >        <ImageView            android:layout_width="25dp"            android:layout_height="25dp"            android:layout_centerVertical="true"            android:layout_marginLeft="10dp"            android:src="@drawable/ic_setting" />        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentLeft="true"            android:layout_centerVertical="true"            android:layout_marginLeft="70dp"            android:text="@string/it_il_personal_setting"            android:textColor="@color/it_normal_textcolor"            android:textSize="@dimen/it_il_personal_list" />        <ImageView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentRight="true"            android:layout_centerVertical="true"            android:layout_marginRight="15dp"            android:src="@drawable/ic_advance" />    </RelativeLayout>    <View        android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_fill" />    <RelativeLayout        android:id="@+id/il_personal_help"        android:layout_width="fill_parent"        android:layout_height="45dp"        android:clickable="true"        android:background="@drawable/layoutclick_style" >        <ImageView            android:layout_width="25dp"            android:layout_height="25dp"            android:layout_centerVertical="true"            android:layout_marginLeft="10dp"            android:src="@drawable/ic_help" />        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentLeft="true"            android:layout_centerVertical="true"            android:layout_marginLeft="70dp"            android:text="反馈建议"            android:textColor="@color/it_normal_textcolor"            android:textSize="@dimen/it_il_personal_list" />        <ImageView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentRight="true"            android:layout_centerVertical="true"            android:layout_marginRight="15dp"            android:src="@drawable/ic_advance" />    </RelativeLayout>    <View        android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_fill" /></LinearLayout>