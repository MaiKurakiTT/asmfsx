<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical" >	    <include         layout="@layout/il_title"        android:layout_width="fill_parent"        android:layout_height="wrap_content"        />    <View         android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_module"        />    <View         android:layout_width="fill_parent"        android:layout_height="10dp"        android:background="@color/view_module"        />    <View         android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_module"        />       <View         android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_notfill"        />      <View         android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_notfill"        />      <View         android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_notfill"        />   <EditText        android:id="@+id/il_feedback_text"       android:layout_width="fill_parent"       android:layout_height="200dp"       android:background="#ffffffff"       android:gravity="top"       android:hint="请输入您的建议..."       />   <View         android:layout_width="fill_parent"        android:layout_height="0.1dp"        android:background="@color/view_notfill"        />   <RelativeLayout         android:id="@+id/il_feedback_ok"        android:layout_width="300dp"        android:layout_height="50dp"         android:background="@drawable/button_style_green"            android:clickable="true"            android:layout_marginTop="30dp"            android:layout_gravity="center_horizontal"        >        <TextView             android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="提交"            android:textColor="@color/it_normal_textcolor"            android:textSize="18dp"            android:layout_centerInParent="true"            />            </RelativeLayout></LinearLayout>